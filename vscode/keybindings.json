// Place your key bindings in this file to overwrite the defaults
[
  { "key": "shift+cmd+v",           "command": "workbench.action.markdown.togglePreview" },
  { "key": "shift+cmd+u",           "command": "workbench.action.output.toggleOutput" },
  { "key": "cmd+j",                 "command": "workbench.action.togglePanel" },
  { "key": "cmd+b",                 "command": "workbench.action.toggleSidebarVisibility" },
  { "key": "cmd+shift+]",           "command": "workbench.action.nextEditor" },
  { "key": "cmd+shift+[",           "command": "workbench.action.previousEditor" },
  { "key": "ctrl+shift+tab",        "command": "workbench.action.openPreviousEditor" },
  { "key": "cmd+ctrl+p",            "command": "workbench.action.openRecent" },
  { "key": "ctrl+tab",              "command": "workbench.action.openPreviousEditorFromHistory " },
  { "key": "shift+cmd+0",           "command": "workbench.action.terminal.toggleTerminal" },
  { "key": "cmd+0",                 "command": "workbench.action.terminal.focus"},
  { "key": "cmd+shift+t",           "command": "go.test.cursor", "args": {"a": 1, "b": 2}},
  { "key": "cmd+shift+,",           "command": "go.test.previous" },
  { "key": "shift+cmd+g",           "command": "workbench.view.scm" },
  { "key": "ctrl+shift+g",          "command": "-workbench.view.scm" },
  { "key": "ctrl+`",                "command": "-workbench.action.terminal.toggleTerminal" },
  { "key": "cmd+numpad_add",        "command": "editor.action.fontZoomIn" },
  { "key": "shift+cmd+=",           "command": "editor.action.fontZoomIn" },
  { "key": "cmd+=",                 "command": "editor.action.fontZoomIn" },
  { "key": "cmd+numpad_subtract",   "command": "editor.action.fontZoomOut" },
  { "key": "shift+cmd+-",           "command": "editor.action.fontZoomOut" },
  { "key": "cmd+-",                 "command": "editor.action.fontZoomOut" },
  { "key": "cmd+numpad0",           "command": "editor.action.fontZoomReset" },
  { "key": "cmd+0",                 "command": "editor.action.fontZoomReset" }
]
